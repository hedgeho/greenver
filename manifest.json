{
  "manifest_version": 3,
  "name": "Sustainability for Cronvall",
  "version": "1.0",
  "description": "sustainability good",
  "content_scripts": [
    {
      "js": ["scripts/alternatives.js"],
      "matches": [
        "https://www.cronvall.fi/epages/CronvallShop.sf/*/?ObjectPath=/Shops/CronvallShop/Products/*"
      ]
    },
    {
      "js": ["scripts/productListIcons.js"],
      "matches": [
        "https://www.cronvall.fi/epages/CronvallShop.sf/*/?ObjectPath=/Shops/CronvallShop/Categories/*"
      ]
    },
    {
      "js": ["scripts/tears.js"],
      "matches": [
        "https://www.cronvall.fi/epages/CronvallShop.sf/*/?ObjectPath=/Shops/CronvallShop/Categories/Tuotteet/Metalli_ja_Muovilevyt/%22Kyynel-%20ja%20kuviolevyt%22"
      ]
    },
    {
      "js": ["scripts/basket.js"],
      "matches": [
        "https://www.cronvall.fi/epages/CronvallShop.sf/*/?ObjectID=*&LastProductObjectID=*"
      ]
    }
  ]
}