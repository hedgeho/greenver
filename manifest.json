{
  "manifest_version": 3,
  "name": "Sustainability for Cronvall",
  "version": "1.0",
  "description": "sustainability good",
  "content_scripts": [
    {
      "js": ["scripts/alternatives.js"],
      "matches": [
        "https://www.cronvall.fi/epages/CronvallShop.sf/fi_FI/?ObjectPath=/Shops/CronvallShop/Products/*"
      ]
    },
    {
      "js": ["scripts/productListIcons.js"],
      "matches": [
        "https://www.cronvall.fi/epages/CronvallShop.sf/fi_FI/?ObjectPath=/Shops/CronvallShop/Categories/*"
      ]
    }
  ]
}